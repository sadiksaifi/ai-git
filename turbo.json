{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".astro/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "check": {
      "dependsOn": ["^check"]
    },
    "check:fix": {
      "dependsOn": ["^check:fix"],
      "cache": false
    },
    "test": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "preview": {
      "cache": false,
      "persistent": true
    },
    "deploy": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "destroy": {
      "cache": false
    },
    "sync:catalog": {
      "cache": false
    },
    "sync:schema": {
      "cache": false
    },
    "sync:models": {
      "cache": false
    }
  }
}
